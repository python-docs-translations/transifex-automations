msgstr ""

#: ../../library/turtle.rst:1721
msgid ""
">>> with turtle.poly():\n"
"...     turtle.forward(100)\n"
"...     turtle.right(60)\n"
"...     turtle.forward(100)"
msgstr ""

#: ../../library/turtle.rst:1734
