msgid "``s[i:j]``"
msgstr "``s[i:j]``"

#: ../../library/stdtypes.rst:1021
msgid "slice of *s* from *i* to *j*"
msgstr "фрагмент *s* від *i* до *j*"

#: ../../library/stdtypes.rst:1021
msgid "(3)(4)"
msgstr "(3)(4)"

#: ../../library/stdtypes.rst:1023
msgid "``s[i:j:k]``"
msgstr "``s[i:j:k]``"

#: ../../library/stdtypes.rst:1023
msgid "slice of *s* from *i* to *j* with step *k*"
msgstr "фрагмент *s* від *i* до *j* з кроком *k*"

#: ../../library/stdtypes.rst:1023
