msgid "Flag"
msgstr "Флаг"

#: ../../c-api/long.rst:541
msgid "Value"
msgstr "Значение"

#: ../../c-api/long.rst:543
msgid "``-1``"
msgstr "``-1``"

#: ../../c-api/long.rst:544
msgid "``0``"
msgstr "``0``"

#: ../../c-api/long.rst:545
msgid "``1``"
msgstr "``1``"

