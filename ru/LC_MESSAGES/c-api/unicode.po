msgstr "*n/a*"

#: ../../c-api/unicode.rst:466
msgid "The literal ``%`` character."
msgstr "Буквальный символ ``%``."

#: ../../c-api/unicode.rst:468
msgid "``d``, ``i``"
msgstr "``d``, ``i``"

#: ../../c-api/unicode.rst:469 ../../c-api/unicode.rst:473
#: ../../c-api/unicode.rst:477 ../../c-api/unicode.rst:481
