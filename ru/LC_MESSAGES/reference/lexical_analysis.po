msgid "Operators"
msgstr "Операторы"

#: ../../reference/lexical_analysis.rst:1361
msgid "The following tokens are operators:"
msgstr "The following tokens are operators:"

#: ../../reference/lexical_analysis.rst:1363
msgid ""
"+       -       *       **      /       //      %      @\n"
"<<      >>      &       |       ^       ~       :=\n"
"<       >       <=      >=      ==      !="
