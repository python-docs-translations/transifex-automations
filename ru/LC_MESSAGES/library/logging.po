msgid "``INFO``"
msgstr "``INFO``"

#: ../../library/logging.rst:397
msgid "20"
msgstr "20"

#: ../../library/logging.rst:399
msgid "``DEBUG``"
msgstr "``DEBUG``"

#: ../../library/logging.rst:399
msgid "10"
msgstr "10"

