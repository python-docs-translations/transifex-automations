#: ../../library/tomllib.rst:71
msgid "Parsing a TOML file::"
msgstr "Parsning av en TOML-fil::"

#: ../../library/tomllib.rst:73
msgid ""
"import tomllib\n"
"\n"
"with open(\"pyproject.toml\", \"rb\") as f:\n"
"    data = tomllib.load(f)"
msgstr ""
"import tomllib\n"
"\n"
"with open(\"pyproject.toml\", \"rb\") as f:\n"
"    data = tomllib.load(f)"

#: ../../library/tomllib.rst:78
msgid "Parsing a TOML string::"
msgstr "Parsning av en TOML-sträng::"

#: ../../library/tomllib.rst:80
msgid ""
"import tomllib\n"
"\n"
"toml_str = \"\"\"\n"
"python-version = \"3.11.0\"\n"
"python-implementation = \"CPython\"\n"
"\"\"\"\n"
"\n"
"data = tomllib.loads(toml_str)"
msgstr ""
"import tomllib\n"
"\n"
"toml_str = \"\"\"\n"
"python-version = \"3.11.0\"\n"
"python-implementation = \"CPython\"\n"
"\"\"\"\n"
"\n"
"data = tomllib.loads(toml_str)"

