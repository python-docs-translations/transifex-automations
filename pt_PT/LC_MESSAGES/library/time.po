msgstr "``%M``"

#: ../../library/time.rst:459
msgid "Minute as a decimal number [00,59]."
msgstr "Minuto como um número decimal [00,59]."

#: ../../library/time.rst:462
msgid "``%p``"
msgstr "``%p``"

#: ../../library/time.rst:462
