">>> '12'.zfill(5)\n"
"'00012'\n"
">>> '-3.14'.zfill(7)\n"
"'-003.14'\n"
">>> '3.14159265359'.zfill(5)\n"
"'3.14159265359'"
msgstr ""
">>> '12'.zfill(5)\n"
"'00012'\n"
">>> '-3,14'.zfill(7)\n"
"'-003,14'\n"
">>> '3,14159265359'.zfill(5)\n"
"'3,14159265359'"

