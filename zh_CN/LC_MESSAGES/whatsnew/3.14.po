"from tempfile import NamedTemporaryFile\n"
"\n"
"with NamedTemporaryFile(mode='w', suffix='.py', delete=False) as f:\n"
"    script_path = f.name\n"
"    f.write(f'import my_debugger; my_debugger.connect({os.getpid()})')\n"
"\n"
